{
    "name": "piedweb/logs-analyzer",
    "type": "library",
    "description": "Can parse, filter and export to CSV via PHP or CLI your APACHE/Nginx/Microsoft/WhatYouWant logs.",
    "keywords": [
        "Pied Web",
        "LogsAnalyzer",
        "Apache",
        "nginx",
        "logs",
        "log",
        "iis",
        "logs parser",
        "parser"
    ],
    "homepage": "https://dev.piedweb.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin D. (ak Pied Web)",
            "email": "contact@robin-d.fr",
            "homepage": "https://piedweb.com"
        }
    ],
    "require": {
        "php" : "~7.1",
        "cheprasov/php-cli-args": "^2.1",
        "kassner/log-parser": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PiedWeb\\LogsAnalyzer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiedWeb\\LogsAnalyzer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
